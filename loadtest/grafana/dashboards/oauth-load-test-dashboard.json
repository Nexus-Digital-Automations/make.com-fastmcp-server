{
  "dashboard": {
    "id": null,
    "title": "OAuth Load Testing Dashboard",
    "tags": ["load-testing", "oauth", "fastmcp"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "5s",
    "schemaVersion": 27,
    "version": 1,
    "panels": [
      {
        "id": 1,
        "title": "OAuth Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(oauth_success_rate[1m])",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                { "color": "red", "value": 0 },
                { "color": "yellow", "value": 0.95 },
                { "color": "green", "value": 0.99 }
              ]
            },
            "unit": "percentunit"
          }
        },
        "gridPos": { "h": 8, "w": 6, "x": 0, "y": 0 }
      },
      {
        "id": 2,
        "title": "OAuth Errors per Second",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(oauth_errors[1m])",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                { "color": "green", "value": 0 },
                { "color": "yellow", "value": 1 },
                { "color": "red", "value": 10 }
              ]
            },
            "unit": "reqps"
          }
        },
        "gridPos": { "h": 8, "w": 6, "x": 6, "y": 0 }
      },
      {
        "id": 3,
        "title": "Active Virtual Users",
        "type": "stat",
        "targets": [
          {
            "expr": "k6_vus",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        },
        "gridPos": { "h": 8, "w": 6, "x": 12, "y": 0 }
      },
      {
        "id": 4,
        "title": "HTTP Requests per Second",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(k6_http_reqs[1m])",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "reqps"
          }
        },
        "gridPos": { "h": 8, "w": 6, "x": 18, "y": 0 }
      },
      {
        "id": 5,
        "title": "OAuth Performance Timeline",
        "type": "timeseries",
        "targets": [
          {
            "expr": "oauth_auth_flow_duration",
            "refId": "A",
            "legendFormat": "Auth Flow Duration"
          },
          {
            "expr": "oauth_token_refresh_duration",
            "refId": "B",
            "legendFormat": "Token Refresh Duration"
          },
          {
            "expr": "oauth_session_validation_duration",
            "refId": "C",
            "legendFormat": "Session Validation Duration"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "ms",
            "thresholds": {
              "steps": [
                { "color": "green", "value": 0 },
                { "color": "yellow", "value": 100 },
                { "color": "red", "value": 500 }
              ]
            }
          }
        },
        "gridPos": { "h": 8, "w": 24, "x": 0, "y": 8 }
      },
      {
        "id": 6,
        "title": "HTTP Response Times",
        "type": "timeseries",
        "targets": [
          {
            "expr": "k6_http_req_duration{quantile=\"0.5\"}",
            "refId": "A",
            "legendFormat": "P50"
          },
          {
            "expr": "k6_http_req_duration{quantile=\"0.95\"}",
            "refId": "B",
            "legendFormat": "P95"
          },
          {
            "expr": "k6_http_req_duration{quantile=\"0.99\"}",
            "refId": "C",
            "legendFormat": "P99"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "ms"
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 }
      },
      {
        "id": 7,
        "title": "HTTP Status Codes",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(k6_http_reqs{status=\"200\"}[1m])",
            "refId": "A",
            "legendFormat": "200 OK"
          },
          {
            "expr": "rate(k6_http_reqs{status=\"401\"}[1m])",
            "refId": "B",
            "legendFormat": "401 Unauthorized"
          },
          {
            "expr": "rate(k6_http_reqs{status=\"500\"}[1m])",
            "refId": "C",
            "legendFormat": "500 Server Error"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "reqps"
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 }
      },
      {
        "id": 8,
        "title": "System Resource Usage",
        "type": "timeseries",
        "targets": [
          {
            "expr": "process_resident_memory_bytes / 1024 / 1024",
            "refId": "A",
            "legendFormat": "Memory Usage (MB)"
          },
          {
            "expr": "rate(process_cpu_seconds_total[1m]) * 100",
            "refId": "B",
            "legendFormat": "CPU Usage (%)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": { "h": 8, "w": 24, "x": 0, "y": 24 }
      }
    ],
    "time": {
      "from": "now-15m",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["1s", "5s", "10s", "30s", "1m", "5m"]
    }
  }
}
